{
  "name": "@snipet/core",
  "version": "1.0.0",
  "description": "",
  "author": "",
  "private": true,
  "license": "Apache-2.0",
  "scripts": {
    "check-types": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "generate:prompts": "node scripts/generate-prompt-types.js",
    "prebuild": "rimraf dist && npm run generate:prompts",
    "build": "nest build",
    "start": "nest start",
    "start:debug": "nest start --debug --watch",
    "prestart:prod": "npm run migrate:prod",
    "start:prod": "node dist/src/main",
    "predev": "npm run generate:prompts",
    "dev": "nest start --watch",
    "predev:development": "npm run generate:prompts",
    "dev:development": "ENV_FILE=.env.development.local nest start --watch",
    "typeorm": "typeorm-ts-node-commonjs -d ./src/data-source.ts",
    "db:generate": "node scripts/generate-migration.js",
    "db:migrate": "pnpm typeorm migration:run",
    "db:migrate:prod": "npm run typeorm migration:run",
    "compodoc": "npx @compodoc/compodoc -p tsconfig.json -s",
    "test": "jest --runInBand",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@bull-board/api": "^6.12.7",
    "@bull-board/express": "^6.12.7",
    "@bull-board/nestjs": "^6.12.7",
    "@google/genai": "^1.30.0",
    "@google/generative-ai": "^0.24.1",
    "@langchain/classic": "^1.0.5",
    "@langchain/community": "^1.0.2",
    "@langchain/core": "^1.0.4",
    "@langchain/textsplitters": "^1.0.1",
    "@nestjs/bullmq": "^11.0.3",
    "@nestjs/common": "^11.0.1",
    "@nestjs/core": "^11.0.1",
    "@nestjs/mapped-types": "*",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/schedule": "^6.0.1",
    "@nestjs/serve-static": "^5.0.3",
    "@nestjs/swagger": "^11.2.1",
    "@nestjs/typeorm": "^11.0.0",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/auto-instrumentations-node": "^0.67.1",
    "@opentelemetry/exporter-trace-otlp-http": "^0.208.0",
    "@opentelemetry/instrumentation": "^0.208.0",
    "@opentelemetry/resources": "^2.2.0",
    "@opentelemetry/sdk-node": "^0.208.0",
    "@opentelemetry/semantic-conventions": "^1.38.0",
    "@opentelemetry/tracing": "^0.24.0",
    "@scalar/nestjs-api-reference": "^1.0.8",
    "@snipet/nest-prompt": "^1.0.1",
    "@snipet/permission": "^1.0.2",
    "@zilliz/milvus2-sdk-node": "^2.6.0",
    "argon2": "^0.44.0",
    "bcrypt": "^6.0.0",
    "bullmq": "^5.58.5",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "cookie-parser": "^1.4.7",
    "dotenv": "^17.2.1",
    "express-basic-auth": "^1.2.1",
    "gray-matter": "^4.0.3",
    "handlebars": "^4.7.8",
    "ioredis": "^5.8.0",
    "langchain": "^1.0.4",
    "mammoth": "^1.11.0",
    "moment": "^2.30.1",
    "nestjs-cls": "^6.0.1",
    "nodemailer": "^7.0.10",
    "officeparser": "^5.2.1",
    "openai": "^6.6.0",
    "pdf-parse": "^1.1.1",
    "pdfjs-dist": "^5.4.54",
    "pg": "^8.16.3",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "stream-to-blob": "^2.0.1",
    "typeorm": "^0.3.27",
    "voyageai": "^0.0.8",
    "word-extractor": "^1.0.4",
    "yaml": "^2.8.2",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@stylistic/eslint-plugin-ts": "*",
    "@types/bcrypt": "^6.0.0",
    "@types/cookie-parser": "^1.4.10",
    "@types/express": "^5.0.0",
    "@types/jest": "^30.0.0",
    "@types/multer": "^2.0.0",
    "@types/node": "^22.10.7",
    "@types/stream-to-blob": "^2.0.0",
    "@types/supertest": "^6.0.2",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^16.0.0",
    "jest": "^30.2.0",
    "prettier": "^3.4.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.4.6",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.20.5",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.20.0"
  },
  "jest": {
    "moduleFileExtensions": ["js", "json", "ts"],
    "modulePaths": ["<rootDir>"],
    "moduleNameMapper": {
      "@/(.*)$": "<rootDir>/src/$1"
    },
    "rootDir": "./",
    "testRegex": ".*\\.spec\\.ts$",

    "preset": "ts-jest/presets/default-esm",

    "transform": {
      "^.+\\.(t|j)s$": ["ts-jest", { "useESM": true }]
    },

    "transformIgnorePatterns": [
      "/node_modules/(?!@snipet/permission)"
    ],

    "extensionsToTreatAsEsm": [".ts"],

    "collectCoverageFrom": ["<rootDir>/src/**/*.ts"],
    "coveragePathIgnorePatterns": [
      "<rootDir>/src/main.ts",
      "<rootDir>/src/app.module.ts",
      "index.ts"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node"
  }
}
